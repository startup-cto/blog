{
  "name": "analytics",
  "version": "1.0.0",
  "private": true,
  "main": "./build",
  "scripts": {
    "build": "run-s build:*",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:copy-lock-file": "shx cp ../common/config/rush/pnpm-lock.yaml ./pnpm-lock.yaml",
    "build:synth": "cdk synth",
    "build:zip": "cd cdk.out && zip -r ../output.zip .",
    "deploy": "cdk deploy --app=\"node -r @swc-node/register bin/deploy.ts\" **",
    "release": "aws s3 cp ./output.zip s3://analyticspipelinestack-bucket83908e77-100gbbob6gjkg/",
    "test": "run-p test:*",
    "test:lint": "eslint src/** bin/**",
    "test:unit": "jest",
    "watch": "cdk watch"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-expressions": "^0.7.3",
    "@lambda-middleware/cors": "^2.0.0",
    "validation": "workspace:*",
    "webpack": "~5.75.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.19",
    "@swc/helpers": "^0.5.10",
    "@swc/jest": "^0.2.23",
    "@swc-node/core": "^1.9.1",
    "@swc-node/register": "^1.5.4",
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "access": "workspace:*",
    "aws-cdk-lib": "^2.173.4",
    "aws-sdk": "^2.1261.0",
    "cdk": "^2.173.4",
    "constructs": "10.4.2",
    "esbuild": "^0.15.15",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-standard-reporter": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "node-fetch": "^2.6.7",
    "prettier": "^2.8.0",
    "s3-pipeline-stack": "workspace:*",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "uuid": "^9.0.0"
  }
}
