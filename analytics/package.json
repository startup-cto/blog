{
  "name": "analytics",
  "version": "1.0.0",
  "private": true,
  "main": "./build",
  "scripts": {
    "build": "run-s build:*",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:copy-lock-file": "shx cp ../common/config/rush/pnpm-lock.yaml ./pnpm-lock.yaml",
    "build:synth": "cdk synth",
    "build:zip": "cd cdk.out && zip -r ../output.zip .",
    "deploy": "cdk deploy --app=\"node -r @swc-node/register bin/deploy.ts\" **",
    "release": "aws s3 cp ./output.zip s3://analyticspipelinestack-bucket83908e77-100gbbob6gjkg/",
    "test": "run-p test:*",
    "test:lint": "eslint src/** bin/**",
    "test:unit": "jest",
    "watch": "cdk watch"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-expressions": "^0.7.3",
    "@lambda-middleware/cors": "^2.0.0",
    "validation": "workspace:*",
    "webpack": "~5.72.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.154",
    "@swc/jest": "^0.2.20",
    "@swc-node/core": "^1.8.2",
    "@swc-node/register": "^1.4.2",
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.24",
    "@types/node-fetch": "~2.6.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "access": "workspace:*",
    "aws-cdk-lib": "^2.42.0",
    "aws-sdk": "^2.1111.0",
    "cdk": "^2.42.0",
    "constructs": "^10.1.106",
    "esbuild": "^0.14.23",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.4",
    "jest-standard-reporter": "^2.0.0",
    "npm-run-all": "~4.1.5",
    "node-fetch": "^2.6.7",
    "prettier": "^2.5.1",
    "s3-pipeline-stack": "workspace:*",
    "shx": "^0.3.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3",
    "uuid": "^8.3.2"
  }
}
